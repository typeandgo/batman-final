    "dev:server": "webpack --progress --config config/webpack.dev.server.js --watch",
    "dev:start": "nodemon --watch ./dist --exec node ./dist/server.js",
    "build": "rimraf dist && webpack --config config/webpack.prod.client.js && webpack --config config/webpack.prod.server.js",
    "start": "node dist/server.js"
  },
